{
  "name": "getto-project-example-ui",
  "version": "0.7.8",
  "description": "",
  "main": "index.js",
  "scripts": {
    "storybook": "cd storybook && npm run build",
    "start:public": "webpack serve --mode development --config example/public/webpack.config.js",
    "start:secure": "webpack serve --mode development --config example/secure/webpack.config.js",
    "build": "npm run setup && npm run webpack",
    "webpack": "npm run webpack:public && npm run webpack:secure",
    "webpack:public": "webpack --mode production --config example/public/webpack.config.js",
    "webpack:secure": "webpack --mode production --config example/secure/webpack.config.js",
    "setup": "npm run setup:protobuf && npm run setup:getto-css && npm run setup:css && npm run setup:environment",
    "setup:protobuf": "sh example/protobuf.sh",
    "setup:getto-css": "sh example/setup-css.sh",
    "setup:css": "npm run postcss:font && npm run postcss:icon",
    "setup:environment": "node example/setup.js",
    "postcss:font": "postcss --config example/postcss.config.js --output example/public/dist/css/font.min.css example/css/font.css",
    "postcss:icon": "postcss --config example/postcss.config.js --output example/public/dist/css/line-icons.min.css example/css/line-icons.css",
    "test": "npm run setup && npm run test:eslint && npm run test:jest",
    "test:eslint": "eslint example/lib/",
    "test:jest": "jest -c example/jest.config.js"
  },
  "author": "shun@getto.systems",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.22.0",
    "htm": "^3.0.4",
    "jest": "^26.6.3",
    "postcss": "^7.0.32",
    "postcss-cli": "^7.1.1",
    "preact": "^10.5.13",
    "protobufjs": "^6.10.2",
    "terser-webpack-plugin": "^5.1.0",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "typescript": "^4.2.3",
    "webpack": "^5.26.3",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "worker-plugin": "^5.0.0"
  }
}
