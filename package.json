{
  "name": "getto-project-example-ui",
  "version": "0.7.8",
  "description": "",
  "main": "index.js",
  "scripts": {
    "storybook": "cd storybook && npm run build",
    "start": "npm run setup && npm run serve",
    "build": "npm run setup && npm run webpack",
    "protobuf": "sh example/protobuf.sh",
    "setup": "node example/setup.js",
    "serve": "npm run serve:public & npm run serve:secure",
    "serve:public": "webpack serve --mode development --config example/public/webpack.config.js",
    "serve:secure": "webpack serve --mode development --config example/secure/webpack.config.js",
    "webpack": "npm run webpack:public && npm run webpack:secure",
    "webpack:public": "webpack --mode production --config example/public/webpack.config.js",
    "webpack:secure": "webpack --mode production --config example/secure/webpack.config.js",
    "postcss": "npm run postcss:font && npm run postcss:icon",
    "postcss:font": "postcss --config example/postcss.config.js --output example/public/dist/css/font.min.css example/css/font.css",
    "postcss:icon": "postcss --config example/postcss.config.js --output example/public/dist/css/line-icons.min.css example/css/line-icons.css",
    "test": "npm run setup && npm run test:eslint && npm run test:jest",
    "test:eslint": "eslint example/lib/",
    "test:jest": "jest -c example/jest.config.js"
  },
  "author": "shun@getto.systems",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.10.0",
    "@typescript-eslint/parser": "^3.10.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.17.0",
    "htm": "^3.0.4",
    "jest": "^26.6.3",
    "postcss": "^7.0.32",
    "postcss-cli": "^7.1.1",
    "preact": "^10.5.12",
    "protobufjs": "^6.10.1",
    "terser-webpack-plugin": "^5.1.0",
    "ts-jest": "^26.4.4",
    "ts-loader": "^7.0.5",
    "typescript": "^4.1.3",
    "typescript-formatter": "^7.2.2",
    "webpack": "^5.13.0",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.1",
    "worker-plugin": "^5.0.0"
  }
}
